{
  "express": {
    "view engine": null
  },
  "view engines": null,
  "databaseConfig": {
    "host": "http://localhost:7474"
  },
  "cdnStore": {
    "path": "C:/Programmieren/Elyoos/server/cdn/data",
    "host": "http://192.168.1.100:8082"
  },
  "bcrypt": {
    "difficulty": 8
  },
  "middleware": {
    "compress": {
      "enabled": true
    },
    "static": {
      "priority": 20,
      "module": {
        "arguments": ["path:./../../client"]
      }
    },
    "json": {
      "priority": 30,
      "module": {
        "name": "body-parser",
        "method": "json",
        "arguments": [
          {
            "type": "application/json"
          }
        ]
      }
    },
    "urlEncoded": {
      "priority": 40,
      "module": {
        "name": "body-parser",
        "method": "urlencoded",
        "arguments": [
          {
            "extended": true
          }
        ]
      }
    },
    "formidable": {
      "priority": 50,
      "module": {
        "name": "formidable"
      }
    },
    "cookieParser": {
      "priority": 60,
      "module": {
        "name": "cookie-parser"
      }
    },
    "session": {
      "priority": 70,
      "module": {
        "name": "express-session"
      }
    },
    "appsec": {
      "enabled": false,
      "priority": 90,
      "module": {
        "name": "lusca"
      }
    },
    "router": {
      "priority": 100,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "path:./controllers"
          }
        ]
      }
    },
    "errorHandling": {
      "enabled": true,
      "priority": 1000,
      "module": {
        "name": "path:./../common/src/lib/error/errors",
        "method": "handlingError"
      }
    }
  }
}
